  <SQLQuery><![CDATA[SELECT ProductDetailsSummary.PILOT_NAME,   
       ProductDetailsSummary.PILOT_ID,   
       ProductDetailsSummary.RECONCILIATED_RLOST,   
       ProductDetailsSummary.SHIPMENT_ID,   
       ProductDetailsSummary.PRODUCT,   
       ProductDetailsSummary.PRODUCT_ID,   
       ProductDetailsSummary.DRIVER_ID,   
       ProductDetailsSummary.OUT_QUANTITY,   
       ProductDetailsSummary.EXPECTED_RETURNED,   
       ProductDetailsSummary.QTY_RETURNED,   
       ProductDetailsSummary.PRODUCT_BALANCE,   
       ProductDetailsSummary.DIRTY_BALANCE,   
       ProductDetailsSummary.PILOT_CLEAN,   
       ProductDetailsSummary.DATA_CLEAN,   
       ProductDetailsSummary.PILOT_DIRTY,   
       ProductDetailsSummary.DATA_DIRTY,   
       ProductDetailsSummary.PILOT_CANCEL,   
       ProductDetailsSummary.DATA_CANCEL,   
       ProductDetailsSummary.PILOT_NEW,   
       ProductDetailsSummary.DATA_NEW,   
       ProductDetailsSummary.LOST_NEW,   
       ProductDetailsSummary.R_LOST,   
       ProductDetailsSummary.NET_RLOST,   
       PRODUCTDETAILSSUMMARY.RLOST_ELIGIBLE   
FROM PRODUCT_DETAILS_SUMMARY PRODUCTDETAILSSUMMARY   
WHERE PRODUCTDETAILSSUMMARY.SHIPMENT_ID = :P_SHIPMENT_ID   
AND ProductDetailsSummary.PRODUCT_ID =  :P_PRODUCT_ID   
UNION ALL   
SELECT 'Total Pilots' AS PILOT_NAME,   
NULL AS PILOT_ID,   
NULL AS RECONCILIATED_RLOST,   
       TotalPilotsProductDet.SHIPMENT_ID,   
       TotalPilotsProductDet.PRODUCT_NAME,   
       TotalPilotsProductDet.PRODUCT_ID,   
       NULL AS DRIVER_ID,   
       TotalPilotsProductDet.OUT_QUANTITY,   
       TotalPilotsProductDet.EXPECTED_RETURNED,   
       TotalPilotsProductDet.QTY_RETURNED,   
       TotalPilotsProductDet.PRODUCT_BALANCE,   
       TOTALPILOTSPRODUCTDET.DIRTY_BALANCE,   
       TOTALPILOTSPRODUCTDET.HUB_CLEAN AS PILOT_CLEAN,   
       TOTALPILOTSPRODUCTDET.DATA_CLEAN,   
       TOTALPILOTSPRODUCTDET.HUB_DIRTY AS PILOT_DIRTY,   
       TOTALPILOTSPRODUCTDET.DATA_DIRTY,   
       TotalPilotsProductDet.HUB_CANCEL AS PILOT_CANCEL,   
       TOTALPILOTSPRODUCTDET.DATA_CANCEL,   
       TotalPilotsProductDet.HUB_NEW AS PILOT_NEW,   
       TotalPilotsProductDet.DATA_NEW,   
       TOTALPILOTSPRODUCTDET.LOST_NEW,   
       HUB_RLOST AS R_LOST,   
       TotalPilotsProductDet.NET_RLOST,   
       TotalPilotsProductDet.RLOST_ELEGIBLE   
FROM TOTAL_PILOTS_PRODUCT_DET TOTALPILOTSPRODUCTDET   
WHERE TOTALPILOTSPRODUCTDET.SHIPMENT_ID = :P_SHIPMENT_ID   
AND TotalPilotsProductDet.PRODUCT_ID = :P_PRODUCT_ID 
UNION 
SELECT ProductDetailsSummary.PILOT_NAME,   
       ProductDetailsSummary.PILOT_ID,   
       ProductDetailsSummary.RECONCILIATED_RLOST,   
       ProductDetailsSummary.SHIPMENT_ID,   
       ProductDetailsSummary.PRODUCT,   
       ProductDetailsSummary.PRODUCT_ID,   
       ProductDetailsSummary.DRIVER_ID,   
       ProductDetailsSummary.OUT_QUANTITY,   
       ProductDetailsSummary.EXPECTED_RETURNED,   
       ProductDetailsSummary.QTY_RETURNED,   
       ProductDetailsSummary.PRODUCT_BALANCE,   
       ProductDetailsSummary.DIRTY_BALANCE,   
       ProductDetailsSummary.PILOT_CLEAN,   
       ProductDetailsSummary.DATA_CLEAN,   
       ProductDetailsSummary.PILOT_DIRTY,   
       ProductDetailsSummary.DATA_DIRTY,   
       ProductDetailsSummary.PILOT_CANCEL,   
       ProductDetailsSummary.DATA_CANCEL,   
       ProductDetailsSummary.PILOT_NEW,   
       ProductDetailsSummary.DATA_NEW,   
       ProductDetailsSummary.LOST_NEW,   
       ProductDetailsSummary.R_LOST,   
       ProductDetailsSummary.NET_RLOST,   
       PRODUCTDETAILSSUMMARY.RLOST_ELIGIBLE   
FROM DRIVER_PRODUCT_DETAILS_SUMM PRODUCTDETAILSSUMMARY   
WHERE PRODUCTDETAILSSUMMARY.SHIPMENT_ID = :P_SHIPMENT_ID   
AND ProductDetailsSummary.PRODUCT_ID =  :P_PRODUCT_ID]]></SQLQuery>
