  <SQLQuery><![CDATA[SELECT TO_DATE(CarReconciliationEO.COVER_DATE,'DD/MM/YYYY') AS COVER_DATE,           
       CarReconciliationEO.DATA_AGENT,           
       CarReconciliationEO.DATA_STATUS,           
       CarReconciliationEO.DRIVER_NAME,           
       CARRECONCILIATIONEO.OPERATION_AGENT,           
       TO_DATE(TO_DATE(CarReconciliationEO.RETURN_DATE,'DD/MM/YYYY hh24:mi:ss')) AS RETURN_DATE,           
       CarReconciliationEO.SHIPMENT_ID,           
       CARRECONCILIATIONEO.STATUS_NAME,           
       TO_DATE(CarReconciliationEO.TO_COVER_DATE,'DD/MM/YYYY') AS  TO_COVER_DATE ,      
       NVL(CARRECONCILIATIONEO.PILOT_COVER_EDITED,0) AS PILOT_COVER_EDITED,      
       CARRECONCILIATIONEO.PILOT_COVER_COMMENT  ,     
       CARRECONCILIATIONEO.CAR_STATUS_ID ,  
       CarReconciliationEO.SHIPMENT_DATE,  
       CarReconciliationEO.TO_COVER_DATE_,  
       CarReconciliationEO.COVER_DATE_ 
FROM  CAR_RECONCILIATION_VIEW CARRECONCILIATIONEO         
WHERE CarReconciliationEO.OPERATION_AGENT = :P_CURRENT_USER   
AND CARRECONCILIATIONEO.DATA_STATUS_ID >1]]></SQLQuery>
